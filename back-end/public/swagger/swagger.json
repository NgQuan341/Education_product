{
    "swagger": "2.0",
    "info": {
        "title": "Laravel and Swagger",
        "description": "Getting started with Laravel and Swagger",
        "termsOfService": "",
        "contact": {
            "email": "name@example.com"
        },
        "version": "1.0.0"
    },
    "host": "localhost:8000",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "paths": {
        "api/user/login/": {
            "post": {
                "description": "Return a user's information",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Your email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Your password",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "id": {
                                    "description": "UUID",
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "email_verified_at": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                },
                                "avatar": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Login kh\u00f4ng th\u00e0nh c\u00f4ng!"
                    }
                }
            }
        },
        "api/user/register/": {
            "post": {
                "description": "Return a user's information",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Your email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Your name",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Your password",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "confirm_password",
                        "in": "query",
                        "description": "Your confirm_password",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "remember_token": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Missing Data"
                    }
                }
            }
        },
        "api/inbox/": {
            "post": {
                "description": "Return ID account logining, messages of login account ",
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "query",
                        "description": "ID account logining",
                        "required": false,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "user_id":{
                                    "type":"integer"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "remember_token": {
                                    "type": "string"
                                },
                                "is_admin": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    }                   
                }
            }
        },
        "api/inbox_admin/": {
            "post": {
                "description": "Return all users and messages",
                "request": [
                    {
                        "name": "user_id",
                        "in": "query",
                        "description": "ID account logining",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "sender_id",
                        "in": "query",
                        "description": "ID account send message",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "remember_token": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Get wrong"
                    }
                   
                }
            }
        },
        "api/sendmess/": {
            "post": {
                "description": "Return messages",
                "request": [
                    {
                        "name": "user_id",
                        "in": "query",
                        "description": "ID account logining",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "sender_id",
                        "in": "query",
                        "description": "ID account send message",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "message",
                        "in": "query",
                        "description": "Content of message",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "message": {
                                    "type": "string"
                                },
                                "user_id": {
                                    "type": "string"
                                },
                                "receiver": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Send message error"
                    }
                   
                }
            }
        }
    },
    "definitions": {}
}