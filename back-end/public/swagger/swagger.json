                                },
                                "avatar": {
                                    "type": "string"
                                },
                                "nameAccount": {
                                    "type": "string"
                                },
                                "linkFB": {
                                    "type": "string"
                                },
                                "phone": {
                                    "type": "string"
                                },
                                "address": {
                                    "type": "string"
                                },
                                "birthday": {
                                    "type": "datetime"
                                },
                                "sex": {
                                    "type": "string"
                                },
                                "status": {
                                    "type": "string"
                                }
                            }
